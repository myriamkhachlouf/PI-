{% extends 'base.html.twig' %}
{% block title %}Formateur{% endblock %}
{% block content %}
    <h1>Formateur</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ formateur.id }}</td>
        </tr>
        <tr>
            <th>Nom</th>
            <td>{{ formateur.nom }}</td>
        </tr>
        <tr>
            <th>Prenom</th>
            <td>{{ formateur.prenom }}</td>
        </tr>
        <tr>
            <th>Statut</th>
            <td>{{ formateur.statut }}</td>
        </tr>
        <tr>
            <th>Typecontrat</th>
            <td>{{ formateur.typecontrat }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ formateur.email }}</td>
        </tr>
        <tr>
            <th>Password</th>
            <td>{{ formateur.password }}</td>
        </tr>
        </tbody>
    </table>

    <a href="{{ path('formateur_index') }}">back to list</a>

    <a href="{{ path('formateur_edit', {'id': formateur.id}) }}">edit</a>

    {{ include('formateur/_delete_form.html.twig') }}

{% endblock %}
