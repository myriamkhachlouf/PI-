
<a href="" class="tooltip-test" data-placement="top" title="" data-toggle="modal" data-target="#exampleModall{{ coupon.id }}">
    <span class="fa fa-edit" aria-hidden="true"></span> Modifier</a>
<div class="modal fade" id="exampleModall{{ coupon.id  }}"  tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <br><br><br>
        <div class="modal-content">
            <div class="modal-header">

                <h4 class="modal-title" id="exampleModalLabel">Modifier :</h4>
            </div>
            <div class="modal-body">

                {{ form_start(edit_form) }}



                <label for="recipient-name" class="form-control-label" style="color: black;" >Objectif :</label>
                {{ form_widget(edit_form.objectif) }}


            </div>
            <div class="modal-footer" >
                <div align="center">
                    <button type="button" class="btn btn-success" data-dismiss="modal">Annuler</button>
                    {{ form_widget(edit_form.submit,{'attr':{'class':'btn btn-warning'}}) }}
                </div>
            </div>
            {{ form_rest(edit_form) }}
            {{ form_end(edit_form) }}

        </div>
    </div>


</div>
